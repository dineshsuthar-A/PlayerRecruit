    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#004E75"
      },
      "package": "com.BVRTech.PlayerRecruit"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}