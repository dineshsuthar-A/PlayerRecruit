{
  "expo": {
    "name": "PlayerRecruit",
    "slug": "PlayerRecruit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splas.png",
      "resizeMode": "contain",
      "backgroundColor": "#004E75"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
